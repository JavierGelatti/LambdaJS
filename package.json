{
  "name": "lambdajs",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "arcsecond": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@pmoo/testy": "^3.1.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "node-sass": "^4.11.0",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "build_dev_sass": "node-sass styles/styles.scss --source-map-embed --output .",
    "build_sass": "node-sass styles/styles.scss --output-style compressed --output .",
    "build_dev_js": "browserify -d -t [ babelify --global --presets [ @babel/preset-env ] ] --outfile bundle.js script.js",
    "dev_build": "npm run build_dev_js && npm run build_dev_sass",
    "uglify": "uglifyjs --compress --mangle --output bundle.js -- bundle.js",
    "build": "npm run build_dev_js && npm run uglify && npm run build_sass",
    "test": "node tests.js"
  },
  "author": "Facundo Javier Gelatti",
  "license": "ISC"
}
